{
    "name": "ts-product-defining-lan",
    "displayName": "TS-PRODUCT Defining Language",
    "version": "1.0.0",
    "publisher": "Coolis1362",
    "description": "This Was Made to Define A Product Description",
    "engines": {
        "vscode": ">=1.0.0"
    },
    "categories": ["Programming Languages", "Snippets"],
    "contributes": {
        "languages": [
            {
                "id": "ts-product-defining-lan",
                "aliases": ["TS-PRODUCT Defining Language", "ts-product-defining-lan"],
                "extensions": [".tsproduct", "product"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ts-product-defining-lan",
                "scopeName": "source.ts",
                "path": "./grammer.json"
            }
        ],
        "snippets": [
            {
                "language": "tskernel-user-defining-lang",
                "path": "./snippets.json"
            }
        ]
    }
}